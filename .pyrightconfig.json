{
  "include": ["src/buddy_gym_bot"],
  "exclude": [
    ".venv",
    "**/__pycache__",
    ".git",
    "src/buddy_gym_bot/migrations",
    "**/site-packages",
    "**/dist",
    "**/build",
    "webapp",
    "infra",
    "migrations",
    "*.pyc",
    "*.pyo",
    "*.pyd",
    ".pytest_cache",
    ".ruff_cache",
    "node_modules",
    "*.egg-info",
    "tests",
    "**/test_*.py",
    "**/tests/**",
    "test_*.py"
  ],
  "pythonVersion": "3.13",
  "pythonPath": ".venv/bin/python3",
  "venvPath": ".",
  "venv": ".venv",
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportOptionalMemberAccess": "warning",
  "reportUnknownMemberType": "warning",
  "useLibraryCodeForTypes": true,
  "extraPaths": [".venv/lib/python3.13/site-packages"]
}
